Utils = require 'utils'
exports.renderJsp = ( options )->
	arr = options.FUNCTION.split '/'
	funcName = arr[arr.length-1]
	jsname = funcName.toLowerCase()
	return """
		<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8" buffer="none"%>
		<%@ taglib uri="/tags/v6fn" prefix="v6fn"%>
		<link rel="stylesheet" href="/isrs/dist/#{options.APP}/#{jsname}/main.css" type="text/css" />
		<script type="text/javascript" src="/isrs/dist/vendor/lodash_backbone.js"></script>
		<script type="text/javascript" src="/isrs/dist/#{options.APP}/#{jsname}/main.js"></script>

		<h3 class="page-title"><span class="page-name">xxxx</span><span class="page-pic"></span></h3>
		<div class="row">
			<div class="col-md-12 col-sm-12">
				<div class="portlet light " style="padding:0;">
					<div class="portlet-title">
						<form class="form-inline">
							<div class="form-group"> 
								<label>月份：</label>
								<div id="month-box" class="input-group date form_month " data-date=""  data-date-format="yyyy-mm"  data-link-format="yyyymm" data-link-field="month-id">
									<input class="form-control" size="16" type="text"  readonly>
									<span class="input-group-addon"><span class="glyphicon glyphicon-th"></span></span>
								</div>
								<input type="hidden" id="month-id" value="${dataMonth}"  query-attr="month" />
							</div>
						</form>
					</div>
					<div class="portlet-body">
						<div class="mt-element-ribbon infobox">
							<div class="ribbon ribbon-vertical-left ribbon-shadow ribbon-color-primary uppercase">
								<div class="ribbon-sub ribbon-bookmark"></div>
								<i class="fa fa-star"></i>
							</div>
							<p class="ribbon-content">
								<span id="abstract"></span>
							</p>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="row">
			<div class="col-md-12 col-sm-12">
				<div class="portlet light " style="padding:0;">
					<div id="table-cont" ></div>	
				</div>
			</div>
		</div>

		<div class="mt-element-ribbon  notesbox">
			<div class="ribbon ribbon-shadow ribbon-color-primary uppercase">
			<span class="icon-book-open" aria-hidden="true"></span>
				&nbsp;&nbsp;说&nbsp;明
			</div>
			<p class="ribbon-content">
				数据来源：省级卷烟营销平台上线运行单位<br>
			</p>
		</div>

	"""